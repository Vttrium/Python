window.plugins["1609275672991x850405320809513000"] = {"human":"URL Modifier w/Parameters","dependencies":{"use_jquery":false,"plugin_api_version":2},"plugin_api":{"human":"New API"},"plugin_actions":{"AAE":{"display":"Update URL","type":"client_side","fields":{"AAF":{"caption":"Parameters (optional)","name":"keyvalues","rank":1,"editor":"key_value_list","value":"text"},"AAH":{"caption":"title (optional)","name":"title","rank":4,"editor":"DynamicValue","value":"text","optional":true},"AAM":{"caption":"new URL (optional)","name":"url","rank":0,"editor":"DynamicValue","value":"text","is_list":false,"optional":true},"AAS":{"caption":"save to browser history","name":"save_history","rank":5,"editor":"DynamicValue","value":"boolean","default_val":true},"AAT":{"caption":"manage parameters","name":"manage_params","rank":2,"editor":"DynamicValue","value":"boolean","default_val":true},"AAW":{"caption":"blank value action","name":"blank_action","rank":3,"editor":"Dropdown","default_val":"Delete","optional":false}},"code":{"client":{"fn":"function(properties, context) {  \n\nclass UrlParams {\n   constructor(search) {\n      this.qs = search || \"\";\n      this.params = {};\n      this.parseQuerstring();\n   }\n   parseQuerstring() {\n      this.qs.split('&').reduce((a, b) => {\n        let [key, val] = b.split('=');\n        a[key] = val;\n        return a;\n      }, this.params);\n   }\n   get(key) {\n      return this.params[key];\n   }\n   toString(){\n\t   let kvs = [];\n      Object.keys(this.params).forEach( key => {\n         if (key) kvs.push(`${key}=${this.params[key]}`);\n      });\n      \n\t   return kvs.join(\"&\")\n   }\n   set(key,val){\n\t   this.params[key] = val;\n   }\n   delete (key){\n\t   delete this.params[key];\t\n   }\n}\n\n\nlet urlStr = properties.url || window.location.href;\nlet domainPath = urlStr.split('?')[0];\nlet newParams;    \nlet pStr = window.location.href.split('?')[1];\nlet params = new UrlParams(pStr)\n\nfor (let i of properties.keyvalues){\n    if (i.value == \"_DEL_\") {params.delete(i.key); console.log('del found')}\n    else if ( i.value == undefined ){\n        if (properties.blank_action == 'Delete')  params.delete(i.key);\n        if (properties.blank_action == 'Blank Value' ) params.set(i.key, \"\");\n        if (properties.blank_action == 'Value Unchanged'); //no action\n    }\n    else params.set(i.key, i.value);\n}\nif (!properties.manage_params)  newParams = urlStr.split('?')[1] || \"\";\nelse newParams = params.toString();\n    \nif (properties.title) document.title = properties.title;\n    \nif (newParams.length > 2) newParams = \"?\" + newParams;\n    \nif ( properties.save_history) history.pushState(null, null, domainPath + newParams);\nif (!properties.save_history) history.replaceState(null, null, domainPath + newParams);\n\n\n  \n}"}}}}};
